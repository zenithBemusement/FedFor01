                @Html.DisplayFor(modelItem => item.Loop)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CampsiteAccessible)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CampsiteLongitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CampsiteLatitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedDate)
